{{page-title "Autocomplete"}}
<AuFieldset
    @alignment={{this.alignment}}
    as |f|
>
    <f.content>
      <AuLabel for="search-input">
        Search GitHub Repositories
      </AuLabel>

      <Autocomplete
        id="search-input"
        placeholder="Example: ember"
        @value={{this.repo}}
        @onInput={{this.setRepo}}
        @search={{this.searchRepos}}
        @options={{this.repos}}
      />
  </f.content>
</AuFieldset>